    <style>
        .ag-courses_item {
            -ms-flex-preferred-size: calc(33.33333% - 30px);
            flex-basis: calc(33.33333% - 30px);

            margin: 0 15px 30px;

            overflow: hidden;

            border-radius: 15px;
        }

        .ag-courses-item_link {
            display: block;
            padding: 30px 20px;
            background-color: rgba(198, 184, 184, 0.4);
            box-shadow: 0 0 10px rgba(105, 101, 101, 0.4);

            overflow: hidden;

            position: relative;
        }


        .ag-courses-item_link:hover .ag-courses-item_bg {
            -webkit-transform: scale(10);
            -ms-transform: scale(10);
            transform: scale(10);
        }

        .ag-courses-item_title {
            min-height: 87px;
            margin: 0 0 25px;
            font-family: 'manrope', sans-serif;
            overflow: hidden;

            font-weight: bolder;
            font-size: 20px;
            color: #000000;

            z-index: 2;
            position: relative;
        }
       
        .ag-courses-item_title:hover{
            color:white;
        }

        .ag-courses-item_bg {
            height: 128px;
            width: 128px;
            /* background-color: rgba(62, 49, 120, 0.6); */
            background: linear-gradient(90deg, #3E3178, 55%, #f417de);

            z-index: 1;
            position: absolute;
            top: -75px;
            right: -75px;            
            border-radius: 50%;
            -webkit-transition: all .5s ease;
            -o-transition: all .5s ease;
            transition: all .5s ease;
        }

        a {
            text-decoration: none;
        }
        a:hover{
            color:white;
        }

        .content-container {
            margin-top: 120px;
